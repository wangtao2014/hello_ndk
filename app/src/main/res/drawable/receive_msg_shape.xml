<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="1dp"
        android:color="@color/teal_700"
        />

    <!-- 填充的颜色 -->
    <solid
        android:color="@color/teal_700" />

    <!-- 设置的圆角 -->
    <corners
        android:radius="10dp" />
</shape>